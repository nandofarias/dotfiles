{
  "defmodule_test_block": {
    "prefix": "deft",
    "body": [
      "defmodule $1Test do",
      "  use ExUnit.Case, async: true\n",
      "  $2",
      "end"
    ],
    "description": "Creates a new test module"
  },
  "defmodule_test_liveview_block": {
    "prefix": "deflvtest",
    "body": [
      "defmodule $1Test do",
      "  use $2.ConnCase, async: true\n",
      "  import Phoenix.LiveViewTest\n",
      "  $2",
      "end"
    ],
    "description": "Creates a new live view test module"
  },
  "describe_block": {
    "prefix": "desc",
    "body": ["describe \"$1\" do", "  $2", "end"]
  },
  "test_block": {
    "prefix": "t",
    "body": ["test \"$1\" do", "  $2", "end"],
    "description": "Creates a test block for unit tests"
  },
  "test_state": {
    "prefix": "ts",
    "body": ["test \"$1\", state do", "  $2", "end"],
    "description": "Creates a test block with state"
  },
  "test_conn_block": {
    "prefix": "tconn",
    "body": ["test \"$1\", %{conn: conn} do", "  $2", "end"],
    "description": "Creates a test block for conn cases"
  },
  "inspect": {
    "prefix": "ins",
    "body": "IO.inspect($1)",
    "description": "IO.inspect"
  },
  "pipe_inspect": {
    "prefix": "pins",
    "body": "|> IO.inspect(label: \"$1\")",
    "description": "|> IO.inspect"
  },
  "def_function": {
    "prefix": "def",
    "body": [
      "def $1($2) do",
      "  $3",
      "end"
    ],
    "description": "Creates a new inline function"
  },
  "def_inline": {
    "prefix": "defl",
    "body": "def $1($2), do: $3",
    "description": "Creates a new inline function"
  },
  "defp_inline": {
    "prefix": "defpl",
    "body": "defp $1($2), do: $3",
    "description": "Creates a new private inline function"
  },
  "FIXME: if_else_block": {
    "prefix": "ifel",
    "body": ["if $1 do", "  $2", "else", "  $3", "end"],
    "description": "Inserts a new if and else block"
  },
  "capture_fn": {
    "prefix": "&",
    "body": "&($1)$2",
    "description": "Inserts simple capture function"
  },
  "doc_block": {
    "prefix": "doc",
    "body": ["@doc \"\"\"", "$1", "\"\"\""],
    "description": "Inserts a function documentation block"
  },
  "moduledoc_block": {
    "prefix": "mdoc",
    "body": ["@moduledoc \"\"\"", "$1", "\"\"\""],
    "description": "Inserts a module documentation block"
  },
  "string_interpolation": {
    "prefix": "#",
    "body": "#{$1}$2",
    "description": "Inserts a string interpolation"
  },
  "map_expansion": {
    "prefix": "%",
    "body": "%{$1: $2}$3",
    "description": "Inserts a new map with atom as key"
  },
  "string_map_expansion": {
    "prefix": "%s",
    "body": "%{\"$1\" => $2}$3",
    "description": "Inserts a new map with string as key"
  },
  "ok_tuple_expansion": {
    "prefix": "ok",
    "body": "{:ok, $1}$2",
    "description": "Inserts an ok tuple"
  },
  "error_tuple_expansion": {
    "prefix": "err",
    "body": "{:error, $1}$2",
    "description": "Inserts an error tuple"
  },
  "override_do_end_block": {
    "prefix": "do",
    "body": ["do", "  $1", "end"],
    "description": "Overrides lsp do end completion with proper indentation"
  },
  "graphql_field": {
    "prefix": "gf",
    "body": ["field :$1, type: $2 do", "  $3", "  resolve &4", "end"],
    "description": "Inserts a new graphql field"
  },
  "setup_block": {
    "prefix": "setup",
    "body": ["setup do", "  $1", "  {:ok, %{$2}}", "end"],
    "description": "Test setup block"
  },
  "genserver": {
    "prefix": "genserver",
    "body": [
      "defmodule $1 do",
      "  use GenServer\n",
      "  # Client API\n",
      "  def start_link(opts) do",
      "    GenServer.start_link(__MODULE__, opts, name: __MODULE__)",
      "  end\n",
      "  # Server Callbacks\n",
      "  @impl true",
      "  def init(state) do",
      "    {:ok, state}",
      "  end\n",
      "  @impl true",
      "  def handle_call($2, _from, state) do",
      "    {:reply, $3, state}",
      "  end\n",
      "  @impl true",
      "  def handle_cast($4, state) do",
      "    {:noreply, state}",
      "  end",
      "end"
    ],
    "description": "GenServer module template"
  },
  "liveview": {
    "prefix": "liveview",
    "body": [
      "defmodule $1Live do",
      "  use $2, :live_view\n",
      "  @impl true",
      "  def mount(_params, _session, socket) do",
      "    {:ok, socket}",
      "  end\n",
      "  @impl true",
      "  def handle_event($3, _params, socket) do",
      "    {:noreply, socket}",
      "  end\n",
      "  @impl true",
      "  def render(assigns) do",
      "    ~H\"\"\"",
      "    $4",
      "    \"\"\"",
      "  end",
      "end"
    ],
    "description": "LiveView module template"
  },
  "schema": {
    "prefix": "schema",
    "body": [
      "schema \"$1\" do",
      "  field :$2, $3\n",
      "  timestamps()",
      "end"
    ],
    "description": "Ecto schema block"
  },
  "changeset": {
    "prefix": "changeset",
    "body": [
      "def changeset(%__MODULE__{} = $1, attrs) do",
      "  $1",
      "  |> cast(attrs, [$2])",
      "  |> validate_required([$3])",
      "end"
    ],
    "description": "Ecto changeset function"
  },
  "pipe": {
    "prefix": "pp",
    "body": "|> $1",
    "description": "Pipe operator"
  },
  "defp_function": {
    "prefix": "defp",
    "body": [
      "defp $1($2) do",
      "  $3",
      "end"
    ],
    "description": "Private function"
  }
}
